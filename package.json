{
  "name": "webpack4",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jalon Lee",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --config ./build/webpack.dev.js",
    "build: dev": "cross-env NODE_ENV=prod rm -rf ./dist && webpack --config ./build/webpack.dev.js",
    "build": "rm -rf ./dist && webpack --config ./build/webpack.prod.js",
    "analyz": "set NODE_ENV=production npm_config_report=true && npm run build:prod"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.7.5",
    "babel-loader": "8.0.0-beta.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^4.0.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "style-loader": "^1.1.3",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
